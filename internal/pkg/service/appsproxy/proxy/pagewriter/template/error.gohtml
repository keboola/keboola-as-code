<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <title>Error</title>
</head>
<body>
    {{ if .App }}
        ProjectID: {{.App.ProjectID}}<br>
        AppID: {{.App.ID}}<br>
        AppName: {{.App.Name}}<br>
        AppIDAndName: {{.App.IDAndName}}<br>
    {{ end }}

    Status: {{.Status}}<br>
    Status: {{.StatusText}}<br>
    Details: {{.Details}}<br>
    ExceptionID: {{.ExceptionID}}<br>
    Messages:<br>
    <ul>
    {{range .Messages}}
        <li>{{.}}</li>
    {{end}}
    </ul>
</body>

</html>
