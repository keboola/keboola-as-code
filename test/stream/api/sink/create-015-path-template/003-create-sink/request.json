{
  "path": "/v1/branches/%%TEST_DEFAULT_BRANCH_ID%%/sources/my-source/sinks",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_STORAGE_API_TOKEN%%"
  },
  "body": {
    "sinkId": "my-sink",
    "name": "My Sink",
    "type": "table",
    "table": {
      "type": "keboola",
      "tableId": "in.c-my-bucket.my-table",
      "mapping": {
        "columns": [
          {
            "type": "uuid",
            "name": "id",
            "primaryKey": true
          },
          {
            "type": "path",
            "name": "path",
            "path": "key",
            "defaultValue": "unknown",
            "rawString": true
          },
          {
            "type": "template",
            "name": "jsonnet",
            "template": {
              "language": "jsonnet",
              "content": "Body(\"key\")"
            },
            "rawString": true
          }
        ]
      }
    }
  }
}
