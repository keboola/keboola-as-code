{
  "path": "/v1/branches/%%TEST_DEFAULT_BRANCH_ID%%/sources/my-source/sinks",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json",
    "X-StorageApi-Token": "%%TEST_KBC_MASTER_TOKEN%%"
  },
  "body": {
    "name": "My Sink",
    "type": "table",
    "table": {
      "type": "keboola",
      "tableId": "in.c-my-bucket.my-table",
      "mapping": {
        "columns": [
          {
            "type": "datetime",
            "name": "datetime"
          },
          {
            "type": "headers",
            "name": "headers"
          },
          {
            "type": "body",
            "name": "body"
          },

          {
            "type": "path",
            "name": "path",
            "path": "key"
          },
          {
            "type": "template",
            "name": "jsonnet",
            "template": {
              "language": "jsonnet",
              "content": "Body('my.value')"
            }
          }
        ]
      }
    }
  }
}
